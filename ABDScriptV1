-- Scripts:

local function MNPR_fake_script() -- TPTOOLDMAP.LocalScript 
	local script = Instance.new('LocalScript', TPTOOLDMAP)

	script.Parent.MouseButton1Click:Connect(function()
		game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(0,320,0)
	end)
end
coroutine.wrap(MNPR_fake_script)()
local function FXCBRX_fake_script() -- TextLabel_2.LocalScript 
	local script = Instance.new('LocalScript', TextLabel_2)

	script.Parent.MouseButton1Click:Connect(function()
		game.ReplicatedStorage.Taunt:FireServer(game.Players.LocalPlayer.Character.Head,"rbxassetid://3657899780", 5, 3, 0)
	
		local TSAnim = Instance.new("Animation")
		TSAnim.AnimationId = "rbxassetid://3445811665"
		local TS = game.Players.LocalPlayer.Character.Humanoid:LoadAnimation(TSAnim)
		wait()
		TS:Play()
		game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 50
		wait(4)
		game.ReplicatedStorage.Taunt:FireServer(game.Players.LocalPlayer.Character.Head,"rbxassetid://4693804951", 5, 3, 0)
		wait(4)
		game.ReplicatedStorage.Taunt:FireServer(game.Players.LocalPlayer.Character.Head,"rbxassetid://3651503836", 5, 3, 0)
		wait(6)
		TS:Stop()
		game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 16
	end)
end
coroutine.wrap(FXCBRX_fake_script)()
local function YOGCTVW_fake_script() -- TextLabel_3.LocalScript 
	local script = Instance.new('LocalScript', TextLabel_3)

	script.Parent.MouseButton1Click:Connect(function()
		game.ReplicatedStorage.Timestop:FireServer(5,"jotaroor")
	end)
end
coroutine.wrap(YOGCTVW_fake_script)()
local function LGNWLQC_fake_script() -- TextLabel_4.REJOIN 
	local script = Instance.new('LocalScript', TextLabel_4)

	script.Parent.MouseButton1Click:Connect(function()
		local ts = game:GetService("TeleportService")
		local p = game:GetService("Players").LocalPlayer
		ts:Teleport(game.PlaceId, p)
	end)
	
end
coroutine.wrap(LGNWLQC_fake_script)()
local function TKXDYH_fake_script() -- Frame.DRAG 
	local script = Instance.new('LocalScript', Frame)

	local Drag = script.Parent
	gsCoreGui = game:GetService("CoreGui")
	gsTween = game:GetService("TweenService")
	local UserInputService = game:GetService("UserInputService")
	local dragging
	local dragInput
	local dragStart
	local startPos
	local function update(input)
		local delta = input.Position - dragStart
		local dragTime = 0.02
		local SmoothDrag = {}
		SmoothDrag.Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X, startPos.Y.Scale, startPos.Y.Offset + delta.Y)
		local dragSmoothFunction = gsTween:Create(Drag, TweenInfo.new(dragTime, Enum.EasingStyle.Sine, Enum.EasingDirection.InOut), SmoothDrag)
		dragSmoothFunction:Play()
	end
	Drag.InputBegan:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
			dragging = true
			dragStart = input.Position
			startPos = Drag.Position
			input.Changed:Connect(function()
				if input.UserInputState == Enum.UserInputState.End then
					dragging = false
				end
			end)
		end
	end)
	Drag.InputChanged:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
			dragInput = input
		end
	end)
	UserInputService.InputChanged:Connect(function(input)
		if input == dragInput and dragging and Drag.Size then
			update(input)
		end
	end)
end
coroutine.wrap(TKXDYH_fake_script)()
local function OFFD_fake_script() -- TextBox.LocalScript 
	local script = Instance.new('LocalScript', TextBox)

	script.Parent.FocusLost:connect(function()
		for i,v in pairs(game.Players:GetChildren()) do
			if (string.sub(string.lower(v.Name),1,string.len(script.Parent.Text))) == string.lower(script.Parent.Text) then
				script.Parent.Text = v.Name
			end
		end
	end)
end
coroutine.wrap(OFFD_fake_script)()
local function FLTW_fake_script() -- TextLabel_5.LocalScript 
	local script = Instance.new('LocalScript', TextLabel_5)

	script.Parent.MouseButton1Click:Connect(function()
		for i,v in pairs(game.Players.LocalPlayer.Character.Stand:GetDescendants()) do
			game.ReplicatedStorage.Transparency:FireServer(v, 0)
			game.ReplicatedStorage.Transparency:FireServer(game.Players.LocalPlayer.Character.Stand.StandRarm, 1)
			game.ReplicatedStorage.Transparency:FireServer(game.Players.LocalPlayer.Character.Stand.StandLarm, 1)
			game.ReplicatedStorage.Transparency:FireServer(game.Players.LocalPlayer.Character.Stand.StandHumanoidRootPart, 1)
			local anim = Instance.new("Animation") -- makes an animation
			anim.AnimationId = "rbxassetid://5520982932"
			local playanim = game.Players.LocalPlayer.Character.Humanoid:LoadAnimation(anim)
			playanim:Play()
		end
	end)
end
coroutine.wrap(FLTW_fake_script)()
local function YUFHY_fake_script() -- tptoplayer.LocalScript 
	local script = Instance.new('LocalScript', tptoplayer)

	script.Parent.Parent.TextBox.FocusLost:connect(
		function(enterPressed)
			if enterPressed then
				print(script.Parent.Parent.TextBox.Text)
			end
		end
	)
	
	local PlayerName = script.Parent.Parent.TextBox
	
	script.Parent.MouseButton1Click:Connect(function()
		for i,v in pairs(game.Workspace:GetChildren()) do
			if v.Name == PlayerName.Text then
				game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.HumanoidRootPart.CFrame  * CFrame.new(0,0,2)
			end
		end
		--game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game.Workspace:FindFirstChild(PlayerName).HumanoidRootPart.CFrame * CFrame.new(0,0,0)
	end)
end
coroutine.wrap(YUFHY_fake_script)()
